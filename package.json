{
  "name": "provue-showcase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:web dev:fastify dev:express",
    "dev:web": "next dev",
    "dev:fastify": "ts-node-dev --respawn --transpile-only server/fastify/server.ts",
    "dev:express": "ts-node-dev --respawn --transpile-only server/express/server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:view": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "check": "npm run lint && npm test -- --runInBand && npm run format && npm run build"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fastify": "^5.6.1",
    "next": "^15.5.6",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redis": "^5.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
